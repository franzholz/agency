{
  "name": "jambagecom/agency",
  "type": "typo3-cms-extension",
  "description": "An improved variant of Kasper Skårhøj's Front End User Admin extension",
  "keywords": [
    "TYPO3",
    "extension",
    "frontend user",
    "registration"
  ],
  "support": {
    "issues": "https://github.com/franzholz/agency/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^8.7 || ^9.5",
    "jambagecom/div2007": "^1.11",
  },
  "suggest": {
    "typo3/cms-felogin": "^8.7 || ^9.5",
    "typo3/cms-rsaauth": "^8.7 || ^9.5",
    "friendsoftypo3/rdct": "^2.0",
    "friendsoftypo3/typo3db-legacy": "^1.1",
    "sjbr/static-info-tables": "^6.7,^6.8,^6.9"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\Agency\\": "Classes"
    }
  },
  "replace": {
    "jambagecom/agency": "self.version",
    "typo3-ter/agency": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "agency"
    }
  }
}
